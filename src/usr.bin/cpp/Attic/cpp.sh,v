		NSI=yes
d72 4
d81 1
a81 1
		ALST=`echo $ALST | sed -e s/-D__GNUC__//`
d91 2
a92 7
		FOUNDFILES=yes
		if [ $NSI = "no" ]
		then
			INCS="$INCS -I/usr/include"
			NSI=skip
		fi
		eval $CPP $ALST $INCS $OPTS $A || exit $?
d97 1
a97 1
if [ $FOUNDFILES = "no" ]
d100 1
a100 5
	if [ $NSI = "no" ]
	then
		INCS="$INCS -I/usr/include"
	fi
	eval exec $CPP $ALST $INCS $OPTS
@


