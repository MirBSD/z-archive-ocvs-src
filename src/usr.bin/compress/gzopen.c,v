static int get_header(gz_stream *);
a108 18
int
gz_check_header(fd, sb, ofn)
	int fd;
	struct stat *sb;
	const char *ofn;
{
	int f;
	u_char buf[sizeof(gz_magic)];
	off_t off = lseek(fd, 0, SEEK_CUR);

	f = (read(fd, buf, sizeof(buf)) == sizeof(buf) &&
	     !memcmp(buf, gz_magic, sizeof(buf)));

	lseek (fd, off, SEEK_SET);

	return f;
}

