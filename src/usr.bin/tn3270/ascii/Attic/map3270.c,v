#ifndef lint
/*static char sccsid[] = "from: @@(#)map3270.c	4.2 (Berkeley) 4/26/91";*/
static char rcsid[] = "$OpenBSD: map3270.c,v 1.4 2001/11/19 19:02:16 mpech Exp $";
