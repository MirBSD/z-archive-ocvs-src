#	$OpenBSD: Makefile,v 1.10 2001/07/30 11:42:09 hugh Exp $
d4 3
a6 11
PROG=	netstat
SRCS=	if.c inet.c inet6.c ipx.c iso.c main.c mbuf.c mroute.c ns.c route.c \
	tp_astring.c unix.c atalk.c mroute6.c
.PATH:	${.CURDIR}/../../sys/netiso
