#ifndef lint
#if 0
static char RCSid[] = 
"$From: child.c,v 6.28 1996/02/22 19:30:09 mcooper Exp $";
#else
static char RCSid[] = 
"$OpenBSD: child.c,v 1.11 2002/06/12 06:07:16 mpech Exp $";
#endif

static char sccsid[] = "@@(#)docmd.c	5.1 (Berkeley) 6/6/85";

