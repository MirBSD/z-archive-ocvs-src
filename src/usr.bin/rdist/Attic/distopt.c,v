#ifndef lint
#if 0
static char RCSid[] = 
"$From: distopt.c,v 6.10 1996/01/30 01:52:07 mcooper Exp $";
#else
static char RCSid[] = 
"$OpenBSD: distopt.c,v 1.6 2001/11/19 19:02:15 mpech Exp $";
#endif

