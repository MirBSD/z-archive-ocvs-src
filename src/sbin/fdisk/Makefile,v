.if ${MACHINE} == "i386" || ${MACHINE} == "macppc" || ${MACHINE} == "alpha"
PROG=	fdisk
SRCS=	fdisk.c user.c misc.c disk.c mbr.c part.c cmd.c manual.c
DPADD=	${LIBUTIL}
LDADD=	-lutil
CLEANFILES+=	manual.c
d3 11
a13 1
MBRPATH=${.OBJDIR}/../bootsys/common/dosmbr
a14 1
CPPFLAGS+= -I${MBRPATH}
d16 3
a18 1
.include <bsd.own.mk>
d20 2
a21 10
.ifdef NOMAN
manual.c:
	(echo 'char manpage[] = "\\'; \
	echo "no manual"; \
	echo '";' ) > manual.c
.else
manual.c:	fdisk.cat8
	(echo 'char manpage[] = "\\'; \
	sed -e 's/[\\"]/\\&/g' -e 's/$$/\\n\\/' fdisk.cat8; \
	echo '";' ) | col -b > manual.c
d24 4
d29 2
a30 1
NOPROG=yes
d32 1
d35 1
a35 1
MANSUBDIR=	i386 macppc
@


