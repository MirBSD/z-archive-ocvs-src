1.3
log
@a non-failing newfs... (at least not more than OpenBSD's)
@
text
@# $MirBSD: src/sbin/newfs/Makefile,v 1.2 2004/04/26 21:55:31 tg Exp $
# $OpenBSD: Makefile,v 1.6 2003/05/06 04:32:16 tedu Exp $

PROG=	newfs
SRCS=	dkcksum.c getmntopts.c newfs.c mkfs.c
MAN=	newfs.8

MOUNT=	${.CURDIR}/../mount
.PATH:	${MOUNT} ${.CURDIR}/../disklabel

CPPFLAGS+=	-DMFS -I${MOUNT}
CFLAGS+=	-Wall -Werror

DPADD=	${LIBUTIL}
LDADD=	-lutil

LINKS=	${BINDIR}/newfs ${BINDIR}/mount_mfs
MLINKS=	newfs.8 mount_mfs.8 newfs.8 mfs.8

.include <bsd.prog.mk>
@


1.2
log
@get rid of warnings
@
text
@d1 1
a1 1
# $MirBSD$
a8 2
CPPFLAGS+=-DMFS -I${MOUNT}
CFLAGS+=-Wall
d10 3
@


1.1
log
@Initial revision
@
text
@d1 2
a2 1
#	$OpenBSD: Makefile,v 1.5 1997/09/21 11:37:41 deraadt Exp $
d9 2
a10 1
CFLAGS+=-DMFS -DFSIRAND -I${MOUNT}
@


