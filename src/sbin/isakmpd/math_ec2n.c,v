			return -1;
		return b2n_set_null(d->y);
a378 4
	b2n_init(h);
	b2n_init(k);
	ec2np_init(q);
	ec2np_init(mina);
d380 1
a380 8
	if (ec2np_set(q, a))
		goto fail;

	/* Create the point -a.  */
	if (ec2np_set(mina, a))
		goto fail;
