.

# ISAKMP DOI Identifier.
ISAKMP_DOI
  ISAKMP		0
.

# ISAKMP Protocol ID.
ISAKMP_PROTO
  ISAKMP		1
.

# ISAKMP transaction message type.
ISAKMP_CFG
  REQUEST		1
  REPLY			2
  SET			3
  ACK			4
  FUTURE_MIN		5
  FUTURE_MAX		127
  PRIVATE_MIN		128
