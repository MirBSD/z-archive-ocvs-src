1.2
log
@* move setting -Os to separate file
* fix order of COPTS assignment, so that -Os is added after
  any custom (mk.conf) COPTS (such as our new -O2)
* some CFLAGS/COPTS -> CPPFLAGS conversions
* nuke junk
@
text
@#	$MirBSD$
#	$OpenBSD: Makefile,v 1.8 2003/05/29 17:33:53 deraadt Exp $

PROG=	more
MAN=
CFLAGS+=-D_USE_OLD_CURSES_ -I.
DPADD=	${LIBOCURSES}
LDADD=	-locurses
CLEANFILES+=morehelp.h

beforedepend: morehelp.h

morehelp.h: ${.CURDIR}/more.help
	echo -n 'const char *more_help = "' > morehelp.h
	sed -e 's/[\\"]/\\&/g' -e 's/$$/\\n\\/' ${.CURDIR}/more.help >> morehelp.h
	echo '";' >> morehelp.h

more.o: morehelp.h

.include <bsd.prog.mk>
@


