1.2.2.2
log
@fix up / document better the $REL_NO_* variables for sparc target
@
text
@d1 1
a1 1
#	$MirBSD: Makefile,v 1.2.2.1 2003/10/23 21:31:33 tg Exp $
d4 1
a4 10
SUBDIR=

.include <bsd.own.mk>

.if !defined(REL_NO_RAMDISK) || ${REL_NO_RAMDISK:L} != "yes" || make(obj) || make(clean)
SUBDIR+= ../ramdisk
.endif

# ../miniroot/Makefile must exist for this
#.if !defined(REL_NO_MINIROOT) || ${REL_NO_MINIROOT:L} != "yes" || make(obj) || make(clean)
a5 8
#.endif

.if !defined(REL_NO_AUTOISO) || ${REL_NO_AUTOISO:L} != "yes" || make(obj) || make(clean)
SUBDIR+= cdfs
.endif

unconfig:
	cd ../ramdisk; ${MAKE} unconfig
@


