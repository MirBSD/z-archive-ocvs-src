1.3
log
@second (piecewise) attempt to merge MirBSD #7ter into HEAD
@
text
@d1 24
a24 2
#	$MirBSD: Makefile,v 1.2.2.4 2003/11/09 17:27:43 tg Exp $
#	$OpenBSD: Makefile,v 1.6 2003/03/05 23:17:20 deraadt Exp $
d28 3
a30 1
.include <bsd.own.mk>
d32 2
a33 2
.if !defined(REL_NO_RAMDISK) || ${REL_NO_RAMDISK:L} != "yes" || make(obj) || make(clean)
SUBDIR+= ../ramdisk
d36 2
a37 2
.if !defined(REL_NO_MINIROOT) || ${REL_NO_MINIROOT:L} != "yes" || make(obj) || make(clean)
SUBDIR+= ../miniroot
d40 2
a41 3
.if !defined(REL_NO_AUTOISO) || ${REL_NO_AUTOISO:L} != "yes" || make(obj) || make(clean)
SUBDIR+= cdfs
.endif
d44 1
a44 1
	cd ../ramdisk; ${MAKE} unconfig
@


