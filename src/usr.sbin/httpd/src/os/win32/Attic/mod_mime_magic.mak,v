!ENDIF 

!IF "$(RECURSE)" == "1" 
CLEAN :"ApacheCore - Win32 DebugCLEAN" 
!ELSE 
CLEAN :
!ENDIF 
	-@@erase "$(INTDIR)\mod_mime_magic.idb"
	-@@erase "$(INTDIR)\mod_mime_magic.obj"
