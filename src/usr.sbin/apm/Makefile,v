1.3
log
@no MANSUBDIR for wscons etc. stuff - I wanna them die
@
text
@# $MirBSD: src/usr.sbin/apm/Makefile,v 1.2 2004/03/21 14:45:30 tg Exp $
# $OpenBSD: Makefile,v 1.7 2001/10/03 19:07:58 drahn Exp $

PROG=		apm
SRCS=		apm.c apmsubr.c
.PATH:	${.CURDIR}/../apmd

CPPFLAGS+=	-I${.CURDIR}/../apmd
LINKS=		${BINDIR}/apm ${BINDIR}/zzz

MAN=		apm.8
MLINKS=		apm.8 zzz.8

.include <bsd.prog.mk>
@


