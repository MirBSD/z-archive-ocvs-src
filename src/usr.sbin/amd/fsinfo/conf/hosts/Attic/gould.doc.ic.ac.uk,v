fs /dev/dk3d {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 3
	mount /usr/r+d/r1 {
		exportfs "\
			ssun1.doc.ic.ac.uk \
			ssun2.doc.ic.ac.uk \
		"
	}
}

fs /dev/dk4h {
	fstype = FSTYPE_UFS
	opts = DEFAULT_OPTS
	freq = 1
	passno = 7
	mount /usr/r+d/r2 {
