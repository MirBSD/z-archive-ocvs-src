	}
}
	
fs /dev/dk0h {
	fstype = FSTYPE_UFS
	opts = rw,quota
	freq = 1
	passno = 2
	dumpset = csg_nightly
	mount /home/gould/staff {
		exportfs "\
			achilles \
			thp1 \
			thp2 \
			thp3 \
			thp4 \
			thp5 \
			thp6 \
			thp7 \
