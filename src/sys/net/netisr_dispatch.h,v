a30 3

#ifndef _NET_NETISR_H_
#error <net/netisr.h> must be included before <net/netisr_dispatch.h>
a38 1
#include "ether.h"
a58 3
#ifdef ISO
	DONETISR(NETISR_ISO,clnlintr);
#endif
d62 1
a62 1
#ifdef NATM
a64 1
#include "ppp.h"
a67 1
#include "bridge.h"
a70 1
#include "lpt.h"
d74 3
a76 1
#endif /* _NET_NETISR_DISPATCH_H_ */
@


