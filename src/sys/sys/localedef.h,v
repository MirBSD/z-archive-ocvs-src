#ifndef _SYS_LOCALEDEF_H_
#define _SYS_LOCALEDEF_H_

#include <sys/param.h>
#include <sys/types.h>

typedef struct {
	const char *abday[7];
	const char *day[7];
	const char *abmon[12];
	const char *mon[12];
	const char *am_pm[2];
	const char *d_t_fmt;
	const char *d_fmt;
	const char *t_fmt;
	const char *t_fmt_ampm;
} _TimeLocale;

extern const _TimeLocale  _DefaultTimeLocale;

#endif /* !_SYS_LOCALEDEF_H_ */
@


