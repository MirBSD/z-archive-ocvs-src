    KC(178),			KS_Alt_R,
};

#define KBD_MAP(name, base, map) \
			{ name, base, sizeof(map)/sizeof(keysym_t), map }

const struct wscons_keydesc zskbd_keydesctab[] = {
	KBD_MAP(KB_US,			0,	zskbd_keydesc_us),
	KBD_MAP(KB_US | KB_LK401,	KB_US,	zskbd_keydesc_us_lk401),
	{0, 0, 0, 0}
};

#undef KBD_MAP
#undef KC
