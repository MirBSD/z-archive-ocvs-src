static __inline void
set_le(void *_p, u_int32_t x)
{
	u_int8_t *p = (u_int8_t *)_p;
	p[0] = (x & 0xFF);
	p[1] = ((x >> 8) & 0xFF);
	p[2] = ((x >> 16) & 0xFF);
	p[3] = ((x >> 24) & 0xFF);
}

