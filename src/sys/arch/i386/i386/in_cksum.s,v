@d1 1
a1 1
/*	$OpenBSD: in_cksum.s,v 1.6 2003/04/17 03:42:14 drahn Exp $	*/
d183 1
a183 1
		call	_C_LABEL(printf)	# printf()
d189 1
a189 1
		.align	4
@

