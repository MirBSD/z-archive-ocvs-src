#include "libsa.h"
#include <machine/apmvar.h>
#include <machine/biosvar.h>
#include "debug.h"
#include "ps2probe.h"

volatile struct BIOS_regs	BIOS_regs;

#if defined(DEBUG) && !defined(_TEST)
#define CKPT(c)	(*(u_int16_t*)0xb8148 = 0x4700 + (c))
#else
#define CKPT(c) /* c */
#endif

extern int debug;
int ps2model;

void
machdep()
{
	/* here */	CKPT('0');
	printf("probing:");
#ifndef _TEST
	/* probe for a model number, gateA20() neds ps2model */
	ps2probe();	CKPT('1');
	gateA20(1);	CKPT('2');
	debug_init();	CKPT('3');
#endif
	/* call console init before doing any io */
	cninit();	CKPT('4');
#ifndef _TEST
	apmprobe();	CKPT('5');
	pciprobe();	CKPT('6');
/*	smpprobe();	CKPT('7'); */
	memprobe();	CKPT('8');
	printf("\n");

	diskprobe();	CKPT('9');
#endif
			CKPT('Z');
}
@


