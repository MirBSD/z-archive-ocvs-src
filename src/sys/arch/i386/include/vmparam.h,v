@/*	$OpenBSD: vmparam.h,v 1.27 2003/05/05 17:54:59 drahn Exp $	*/
/*	$NetBSD: vmparam.h,v 1.15 1994/10/27 04:16:34 cgd Exp $	*/

/*-
