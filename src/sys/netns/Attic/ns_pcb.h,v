@/*	$OpenBSD: ns_pcb.h,v 1.2 1996/03/04 08:20:30 niklas Exp $	*/
/*	$NetBSD: ns_pcb.h,v 1.7 1996/02/13 22:14:05 christos Exp $	*/

/*
