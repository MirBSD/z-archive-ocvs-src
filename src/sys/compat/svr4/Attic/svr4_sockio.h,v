@/*	$OpenBSD: svr4_sockio.h,v 1.2 1996/08/02 20:35:41 niklas Exp $	 */
/*	$NetBSD: svr4_sockio.h,v 1.2 1995/07/04 23:00:13 christos Exp $	 */

