DUMMY(fadvise64);		/* #250 */

#define DUMMY_XATTR(s)							\
int									\
linux_sys_ ## s ## xattr(p, v, retval)						\
	struct proc *p;							\
	void *v;							\
	register_t *retval;						\
{									\
	return (ENOATTR);						\
}									
DUMMY_XATTR(set);
DUMMY_XATTR(lset);
DUMMY_XATTR(fset);
DUMMY_XATTR(get);
DUMMY_XATTR(lget);
DUMMY_XATTR(fget);
