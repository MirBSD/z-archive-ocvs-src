@/**	$MirBSD: src/sys/compat/common/vfs_syscalls_25.c,v 1.5 2004/12/03 09:29:26 tg Exp $ */
/*	$OpenBSD: vfs_syscalls_25.c,v 1.6 2003/08/15 20:32:15 tedu Exp $	*/

