#ifndef _NETIPX_SPX_DEBUG_H_
#define	_NETIPX_SPX_DEBUG_H_

struct	spx_debug {
	u_long	sd_time;
	short	sd_act;
	short	sd_ostate;
	caddr_t	sd_cb;
	short	sd_req;
	struct	spx sd_si;
	struct	spxpcb sd_sp;
};

#define	SA_INPUT 	0
#define	SA_OUTPUT	1
#define	SA_USER		2
#define	SA_RESPOND	3
#define	SA_DROP		4

#ifdef SANAMES
char	*spxnames[] =
    { "input", "output", "user", "respond", "drop" };
#endif

#define	SPX_NDEBUG 100
struct	spx_debug spx_debug[SPX_NDEBUG];
int	spx_debx;

#ifdef _KERNEL
extern char *prurequests[];
extern char *sanames[];

void	spx_trace(int act, int ostate, struct spxpcb *sp, struct spx *si,
		       int req);
#endif

#endif /* !_NETIPX_SPX_DEBUG_H_ */
@


