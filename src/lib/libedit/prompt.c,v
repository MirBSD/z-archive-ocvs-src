			p->p_func = prompt_default;
		else
			p->p_func = prompt_default_r;
	} else
		p->p_func = prf;
	p->p_pos.v = 0;
	p->p_pos.h = 0;
	return (0);
}


/* prompt_get():
 *	Retrieve the prompt printing function
 */
protected int
prompt_get(EditLine *el, el_pfunc_t *prf, int op)
{

	if (prf == NULL)
		return (-1);
	if (op == EL_PROMPT)
		*prf = el->el_prompt.p_func;
	else
		*prf = el->el_rprompt.p_func;
	return (0);
}
