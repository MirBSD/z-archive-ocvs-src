#if defined(LIBC_SCCS) && !defined(lint)
static char rcsid[] = "$MirBSD: catgets.c,v 1.2 2003/04/24 14:49:58 tg Exp $";
#endif /* LIBC_SCCS and not lint */

#define _NLS_PRIVATE

#include <errno.h>
#include <stdlib.h>
#include <string.h>
#include <nl_types.h>

char *
_catgets(catd, set_id, msg_id, s)
	nl_catd catd;
	int set_id;
	int msg_id;
	const char *s;
{
	/* NLS sucks */
	return (char *) s;
}

char *
catgets(catd, set_id, msg_id, s)
	nl_catd catd;
	int set_id;
	int msg_id;
	const char *s;
{
	return (char *) s;
}
