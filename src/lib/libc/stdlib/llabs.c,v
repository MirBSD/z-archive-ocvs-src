#if defined(LIBC_SCCS) && !defined(lint)
static char *rcsid = "$OpenBSD: llabs.c,v 1.1 2003/07/21 20:20:04 millert Exp $";
#endif /* LIBC_SCCS and not lint */

#include <stdlib.h>

long long llabs(long long j)
{
	return (j < 0 ? -j : j);
}
