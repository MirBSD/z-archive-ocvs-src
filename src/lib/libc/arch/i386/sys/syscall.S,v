#include "SYS.h"

	.text
#if defined(SYSLIBC_SCCS)
	.asciz "$OpenBSD: syscall.S,v 1.6 2003/06/02 20:18:30 millert Exp $"
#endif /* SYSLIBC_SCCS */

SYSENTRY(syscall)
	pop	%ecx	/* rta */
	pop	%eax	/* syscall number */
	push	%ecx
	int	$0x80
	push	%ecx	/* Keep stack frame consistant */
	jc	err
	ret
err:
#ifdef PIC
	PIC_PROLOGUE             
	movl	PIC_GOT(CERROR), %ecx
	PIC_EPILOGUE
	jmp	*%ecx
#else
	jmp	CERROR
#endif
@


