@a0 1
/*	$OpenBSD: _setjmp.S,v 1.3 2004/05/04 15:57:06 miod Exp $	*/
a37 3
	.register	%g2,#ignore
	.register	%g3,#ignore

d45 1
a45 1
 * by restoring the previous context.
d50 3
a52 2
	stx	%sp, [%o0 + 0x00]	/* store caller's stack pointer */
	stx	%o7, [%o0 + 0x08]	/* ... and return pc */
d54 1
a54 1
	 clr	%o0			! return 0
d57 26
a82 8
	save	%sp, -CC64FSZ, %sp
	flushw
	ldx	[%i0 + 0x00], %fp
	ldx	[%i0 + 0x08], %i7
	mov	1, %i0
	movrnz	%i1, %i1, %i0		! compute v ? v : 1
	ret
	 restore
