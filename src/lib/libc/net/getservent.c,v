struct servent_data _servent_data;	/* shared with getservby{name,port}.c */

void
setservent(int f)
{
	setservent_r(f, &_servent_data);
}

void
endservent(void)
{
	endservent_r(&_servent_data);
}

struct servent *
getservent(void)
{
	static struct servent serv;

	return getservent_r(&serv, &_servent_data);
