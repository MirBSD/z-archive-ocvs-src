		if (rt0->ip6r0_segleft > 23)
			return (-1);
		if (flags == IPV6_RTHDR_STRICT) {
			int c, b;
			c = rt0->ip6r0_segleft / 8;
			b = rt0->ip6r0_segleft % 8;
			rt0->ip6r0_slmap[c] |= (1 << (7 - b));
		}
		break;
	}
	default:
		return (-1);
	}
