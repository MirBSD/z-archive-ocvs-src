s%@@GLIBCPP_BUILD_VERSIONED_SHLIB_FALSE@@%$GLIBCPP_BUILD_VERSIONED_SHLIB_FALSE%g
s%@@OS_INC_SRCDIR@@%$OS_INC_SRCDIR%g
s%@@ATOMICITY_INC_SRCDIR@@%$ATOMICITY_INC_SRCDIR%g
s%@@CPU_LIMITS_INC_SRCDIR@@%$CPU_LIMITS_INC_SRCDIR%g
