#ifdef HAVE_CONFIG_H
#include "config.h"
#endif
#include <sys/types.h>
#include <signal.h>
#include <errno.h>
#include "f2c.h"

/* fixme: bsd, svr1-3 use int, not pid_t */

#ifdef KR_headers
integer G77_kill_0 (pid, signum)
     integer *pid, *signum;
#else
integer G77_kill_0 (const integer *pid, const integer *signum)
#endif
{
#if defined (HAVE_KILL)
  return kill ((pid_t) *pid, *signum) ? errno : 0;
#else
  errno = ENOSYS;
  return -1;
#endif
}
@


