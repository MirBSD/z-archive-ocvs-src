utf8_uni.h:		utf8_uni.tbl		makeuctb.exe
viscii_uni.h:		viscii_uni.tbl		makeuctb.exe

clean:
	rm -f makeuctb.exe makeuctb *.o *un.h *u.h *c.h *i.h

distclean: clean
	-rm -f *.bak

@


