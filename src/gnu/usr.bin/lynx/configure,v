  echo "$ac_t""yes" 1>&6
    ac_tr_lib=HAVE_LIB$(echo gnutls | sed -e 's/[^a-zA-Z0-9_]/_/g' \
    -e 'y/abcdefghijklmnopqrstuvwxyz/ABCDEFGHIJKLMNOPQRSTUVWXYZ/')
  cat >> confdefs.h <<EOF
#define $ac_tr_lib 1
EOF

  LIBS="-lgnutls $LIBS"

else
  echo "$ac_t""no" 1>&6

		cf_ssl_root=/usr/local/gnutls
		if test -d $cf_ssl_root ; then
			test -n "$verbose" && echo "	assume it is in $cf_ssl_root" 1>&6

		else
			{ echo "configure: error: cannot find gnutls library" 1>&2; exit 1; }
		fi
	
fi

	;;
*)
	if test -d $cf_cv_use_libgnutls ; then
		if test -d $cf_cv_use_libgnutls/include ; then
			cf_ssl_root=$cf_cv_use_libgnutls
		elif test -d $cf_cv_use_libgnutls/../include ; then
			cf_ssl_root=$cf_cv_use_libgnutls/..
		else
			{ echo "configure: error: cannot find ssl library under $cf_cv_use_libgnutls" 1>&2; exit 1; }
		fi
	else
		echo "configure: warning: expected a directory: $cf_cv_use_libgnutls" 1>&2
	fi
	;;
esac
echo "$ac_t""$cf_ssl_root" 1>&6
LIBS="$cf_ssl_library $LIBS"

cf_ssl_subincs=yes
if test "$cf_ssl_root" != no ; then
	cf_ssl_library="-L$cf_ssl_root/lib $cf_ssl_library"
	if test -d $cf_ssl_root/include ; then
		
cf_fix_cppflags=no
cf_new_cflags=
cf_new_cppflags=
cf_new_extra_cppflags=

for cf_add_cflags in -I$cf_ssl_root/include
do
case $cf_fix_cppflags in
no)
	case $cf_add_cflags in #(vi
	-undef|-nostdinc*|-I*|-D*|-U*|-E|-P|-C) #(vi
		case $cf_add_cflags in
		-D*)
			cf_tst_cflags=`echo ${cf_add_cflags} |sed -e 's/^-D[^=]*='\''\"[^"]*//'`

			test "${cf_add_cflags}" != "${cf_tst_cflags}" \
			&& test -z "${cf_tst_cflags}" \
			&& cf_fix_cppflags=yes

			if test $cf_fix_cppflags = yes ; then
				cf_new_extra_cppflags="$cf_new_extra_cppflags $cf_add_cflags"
				continue
			elif test "${cf_tst_cflags}" = "\"'" ; then
				cf_new_extra_cppflags="$cf_new_extra_cppflags $cf_add_cflags"
				continue
			fi
			;;
		esac
		case "$CPPFLAGS" in
		*$cf_add_cflags) #(vi
			;;
		*) #(vi
			cf_new_cppflags="$cf_new_cppflags $cf_add_cflags"
			;;
		esac
		;;
	*)
		cf_new_cflags="$cf_new_cflags $cf_add_cflags"
		;;
	esac
	;;
yes)
	cf_new_extra_cppflags="$cf_new_extra_cppflags $cf_add_cflags"

	cf_tst_cflags=`echo ${cf_add_cflags} |sed -e 's/^[^"]*"'\''//'`

	test "${cf_add_cflags}" != "${cf_tst_cflags}" \
	&& test -z "${cf_tst_cflags}" \
	&& cf_fix_cppflags=no
	;;
esac
done

if test -n "$cf_new_cflags" ; then
	
	CFLAGS="$CFLAGS $cf_new_cflags"
fi

if test -n "$cf_new_cppflags" ; then
	
	CPPFLAGS="$cf_new_cppflags $CPPFLAGS"
fi

if test -n "$cf_new_extra_cppflags" ; then
	
	EXTRA_CPPFLAGS="$cf_new_extra_cppflags $EXTRA_CPPFLAGS"
fi




		test -d $cf_ssl_root/include/gnutls || cf_ssl_subincs=no
	fi
fi

echo $ac_n "checking for SSL_connect in -lgnutls-openssl""... $ac_c" 1>&6
echo "configure:6931: checking for SSL_connect in -lgnutls-openssl" >&5
ac_lib_var=$(echo gnutls-openssl'_'SSL_connect | sed 'y%./+-:%__p__%')
if eval "test \"\${ac_cv_lib_$ac_lib_var+set}\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  ac_save_LIBS="$LIBS"
LIBS="-lgnutls-openssl  $LIBS"
cat > conftest.$ac_ext <<EOF
#line 6939 "configure"
#include "confdefs.h"
/* Override any gcc2 internal prototype to avoid an error.  */
/* We use char because int might match the return type of a gcc2
    builtin and then its argument prototype would still apply.  */
char SSL_connect();

int main() {
SSL_connect()
; return 0; }
EOF
if { (eval echo configure:6950: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=yes"
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=no"
fi
rm -f conftest*
LIBS="$ac_save_LIBS"

fi
if eval "test \"$(echo '$ac_cv_lib_'$ac_lib_var)\" = yes"; then
  echo "$ac_t""yes" 1>&6
  LIBS="-lgnutls-openssl $LIBS"
else
  echo "$ac_t""no" 1>&6
echo $ac_n "checking for SSL_connect in -lgnutls-extra""... $ac_c" 1>&6
echo "configure:6969: checking for SSL_connect in -lgnutls-extra" >&5
ac_lib_var=$(echo gnutls-extra'_'SSL_connect | sed 'y%./+-:%__p__%')
if eval "test \"\${ac_cv_lib_$ac_lib_var+set}\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  ac_save_LIBS="$LIBS"
LIBS="-lgnutls-extra  $LIBS"
cat > conftest.$ac_ext <<EOF
#line 6977 "configure"
#include "confdefs.h"
/* Override any gcc2 internal prototype to avoid an error.  */
/* We use char because int might match the return type of a gcc2
    builtin and then its argument prototype would still apply.  */
