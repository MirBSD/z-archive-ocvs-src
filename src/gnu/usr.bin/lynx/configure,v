; return 0; }
EOF
if { (eval echo configure:6988: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=yes"
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  eval "ac_cv_lib_$ac_lib_var=no"
fi
rm -f conftest*
LIBS="$ac_save_LIBS"

fi
if eval "test \"$(echo '$ac_cv_lib_'$ac_lib_var)\" = yes"; then
  echo "$ac_t""yes" 1>&6
  LIBS="-lgnutls-extra $LIBS"
else
  echo "$ac_t""no" 1>&6
{ echo "configure: error: cannot find gnutls openssl functions" 1>&2; exit 1; }
fi

fi


if test "$cf_ssl_subincs" = yes ; then
echo $ac_n "checking for gnutls include directory""... $ac_c" 1>&6
