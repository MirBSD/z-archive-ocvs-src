	elif test -z "$cf_path_args" ; then
		cf_path_args="$cf_temp"
	else
		cf_path_args="$cf_path_args $cf_temp"
	fi
done
IFS="$cf_save_ifs"

if test -n "$cf_path_prog" ; then
	
echo "(line 17544) testing defining path for ${cf_path_prog} ..." 1>&5

	cat >> confdefs.h <<EOF
#define RM_PATH "$cf_path_prog"
