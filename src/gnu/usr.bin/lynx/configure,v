# $Format: "VERSION=$ProjectVersion$"$
VERSION=2.8.6dev.2

test -z "$ALL_LINGUAS" && ALL_LINGUAS=`test -d $srcdir/po && cd $srcdir/po && echo *.po|sed -e 's/\.po//g' -e 's/*//'`
