	LIBS=`echo " $LIBS " | sed -e 's/ -lintl / /' -e 's/^ //' -e 's/ $//'`
      fi

                  if test "$GMSGFMT" != ":"; then
			if $GMSGFMT --statistics /dev/null >/dev/null 2>&1; then
	  : ;
	else
	  echo "$ac_t""found msgfmt program is not GNU msgfmt; ignore it" 1>&6
	  GMSGFMT=":"
	fi
