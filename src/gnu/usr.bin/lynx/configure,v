	 if test "$cf_check_cppflags" != "$CPPFLAGS" ; then
		 test -n "$verbose" && echo "	but keeping change to \$CPPFLAGS" 1>&6

	 fi
	 CFLAGS="$cf_check_flags"
fi
rm -f conftest*
fi


