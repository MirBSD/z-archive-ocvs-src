!HTGroup.obj :	HTGroup.c HTUtils.h
!HTLex.obj :	HTLex.c HTUtils.h
!HTAssoc.obj :	HTAssoc.c HTAssoc.h HTAAUtil.h HTString.h
!HTUU.obj :	HTUU.c HTUU.h HTUtils.h
!HTVMSUtils.obj :	HTVMSUtils.c HTVMSUtils.h HTUtils.h
