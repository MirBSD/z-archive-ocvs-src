#define RCSBASE "$MirBSD: src/gnu/usr.bin/rcs/src/rcsbase.h,v 1.12 2004/12/12 22:20:08 tg Exp $"
a101 1
#if large_memory
a107 1
#		if maps_memory
a108 3
#		else
			FILE *stream;
#		endif
a109 1
#	if maps_memory
a113 7
#	else
		int Igetmore(RILE*);
#		define declarecache register Iptr_type ptr; register RILE *rRILE
