#define	mainArgs	  (argc,argv) int argc; char **argv;
#define	COPYRIGHT_STRING1 "Copyright 1982,1988,1989 Walter F. Tichy, Purdue CS"
#define	COPYRIGHT_STRING2 "Copyright 1990,1991,1992,1993,1994,1995 Paul Eggert"

#ifdef	__DB
#define COPYRIGHT_STRING COPYRIGHT_STRING1 "\\n" COPYRIGHT_STRING2
#else
#define COPYRIGHT_STRING COPYRIGHT_STRING1 "\n" COPYRIGHT_STRING2
#endif

#if defined(__DB)
/* MirOS extension */
#define libId(name,id)	__DB(name,id);
#define	mainProg(n,c,i)	__DB(Copyright,COPYRIGHT_STRING); \
			__DB(baseid,RCSBASE); const char cmdid[] = c; \
			__DB(n,i); int main(int, char **); \
			int main mainArgs
#elif defined(__RCSID)
/* pretty standard */
#define	libId(name,id)	__RCSID(id);
#define	mainProg(n,c,i)	\
	static const char Copyright[] __attribute__((__unused__)) \
	    = COPYRIGHT_STRING, baseid[] __attribute__((__unused__)) \
	    = RCSBASE; const char cmdid[] = c; \
	libId(n,i); int main(int,char**); \
	int main mainArgs
#elif defined(RCS_lint)
#define	libId(name,id)	/* nothing */
#define	mainProg(n,c,i)	int main mainArgs
#else
#define	libId(name,id)	static const char name[] __attribute__((__unused__)) \
			    = id;
#define	mainProg(n,c,i)	\
	static const char Copyright[] __attribute__((__unused__)) \
	    = COPYRIGHT_STRING, baseid[] __attribute__((__unused__)) \
	    = RCSBASE; const char cmdid[] = c; \
	libId(n,i); int main(int,char**); \
	int main mainArgs
#endif


#if has_snprintf
#define	snprintf1(s,n,f,a1)				snprintf((s),(n),(f),(a1))
#define	snprintf2(s,n,f,a1,a2)				snprintf((s),(n),(f),(a1),(a2))
#define	snprintf3(s,n,f,a1,a2,a3)			snprintf((s),(n),(f),(a1),(a2),(a3))
#define	snprintf4(s,n,f,a1,a2,a3,a4)			snprintf((s),(n),(f),(a1),(a2),(a3),(a4))
#define	snprintf5(s,n,f,a1,a2,a3,a4,a5)			snprintf((s),(n),(f),(a1),(a2),(a3),(a4),(a5))
#define	snprintf6(s,n,f,a1,a2,a3,a4,a5,a6)		snprintf((s),(n),(f),(a1),(a2),(a3),(a4),(a5),(a6))
#define	snprintf7(s,n,f,a1,a2,a3,a4,a5,a6,a7)		snprintf((s),(n),(f),(a1),(a2),(a3),(a4),(a5),(a6),(a7))
#define	snprintf8(s,n,f,a1,a2,a3,a4,a5,a6,a7,a8)	snprintf((s),(n),(f),(a1),(a2),(a3),(a4),(a5),(a6),(a7),(a8))
#else
#define	snprintf1(s,n,f,a1)				sprintf((s),(f),(a1))
#define	snprintf2(s,n,f,a1,a2)				sprintf((s),(f),(a1),(a2))
#define	snprintf3(s,n,f,a1,a2,a3)			sprintf((s),(f),(a1),(a2),(a3))
#define	snprintf4(s,n,f,a1,a2,a3,a4)			sprintf((s),(f),(a1),(a2),(a3),(a4))
#define	snprintf5(s,n,f,a1,a2,a3,a4,a5)			sprintf((s),(f),(a1),(a2),(a3),(a4),(a5))
#define	snprintf6(s,n,f,a1,a2,a3,a4,a5,a6)		sprintf((s),(f),(a1),(a2),(a3),(a4),(a5),(a6))
#define	snprintf7(s,n,f,a1,a2,a3,a4,a5,a6,a7)		sprintf((s),(f),(a1),(a2),(a3),(a4),(a5),(a6),(a7))
#define	snprintf8(s,n,f,a1,a2,a3,a4,a5,a6,a7,a8)	sprintf((s),(f),(a1),(a2),(a3),(a4),(a5),(a6),(a7),(a8))
#endif

