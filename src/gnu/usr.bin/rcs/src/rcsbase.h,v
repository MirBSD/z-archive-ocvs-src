#define RCSBASE "$MirBSD: rcsbase.h,v 1.2 2004/07/21 09:38:07 tg Exp $"
d32 2
a248 29

#define mainArgs (argc,argv) int argc; char **argv;
#define	COPYRIGHT_STRING "Copyright 1982,1988,1989 Walter F. Tichy, Purdue CS\nCopyright 1990,1991,1992,1993,1994,1995 Paul Eggert"

#if defined(__DB)
/* MirOS extension */
#define libId(name,id)	__DB(name,id)
#define	mainProg(n,c,i)	__DB(Copyright,COPYRIGHT_STRING); \
			__DB(baseid,RCSBASE); __DB(cmdid,c); \
			__DB(n,i); int main(int, char **); \
			int main mainArgs
#elif defined(__RCSID)
/* pretty standard */
#define	libId(name,id)	__RCSID(id)
#define	mainProg(n,c,i)	static const char Copyright[] = COPYRIGHT_STRING, \
			baseid[] = RCSBASE, cmdid[] = c; \
			libId(n,i); int main P((int,char**)); \
			int main mainArgs
#elif defined(RCS_lint)
#define	libId(name,id)	/* nothing */
#define	mainProg(n,c,i)	int main mainArgs
#else
#define	libId(name,id)	static const char name[] = id;
#define	mainProg(n,c,i)	static const char Copyright[] = COPYRIGHT_STRING, \
			baseid[] = RCSBASE, cmdid[] = c; \
			libId(n,i); int main P((int,char**)); \
			int main mainArgs
#endif

d592 38
@


