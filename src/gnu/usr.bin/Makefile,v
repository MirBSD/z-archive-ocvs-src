SUBDIR+=	dict ssfe tinyirc mkisofs

# anoncvs must be after cvs
SUBDIR+=	anoncvs

# Do these last; texinfo builds the info 'dir' file, perl must be after binutils
SUBDIR+=	perl texinfo
@


