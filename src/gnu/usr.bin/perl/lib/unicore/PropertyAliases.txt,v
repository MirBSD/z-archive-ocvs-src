ea        ; East_Asian_Width
gc        ; General_Category
jg        ; Joining_Group
jt        ; Joining_Type
lb        ; Line_Break
NFC_QC    ; NFC_Quick_Check
NFKC_QC   ; NFKC_Quick_Check
nt        ; Numeric_Type
sc        ; Script

# ================================================
# Binary Properties
# ================================================
