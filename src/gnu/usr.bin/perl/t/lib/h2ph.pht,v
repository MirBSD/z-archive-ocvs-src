@a2 2
no warnings 'redefine';

a50 1
	my(@@REM);
d52 1
a52 1
	@@REM = map { "$_/sys/fcntl.ph" } (grep { not exists($INCD{"$_/sys/fcntl.ph"}) and -f "$_/sys/fcntl.ph" } @@INC);
