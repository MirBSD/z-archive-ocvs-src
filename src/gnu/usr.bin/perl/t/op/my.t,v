@5.8.5
@
text
@d5 1
a5 1
print "1..33\n";
a101 12

#  [perl #29340] optimising away the = () left the padav returning the
# array rather than the contents, leading to 'Bizarre copy of array' error

sub opta { my @@a=() }
sub opth { my %h=() }
eval { my $x = opta };
print "not " if $@@;
print "ok 32\n";
eval { my $x = opth };
print "not " if $@@;
print "ok 33\n";
@

