1.1.1.2
log
@5.8.5
@
text
@d3 1
a3 1
print "1..12\n";
a32 10

my $qcontext = q{
  $q = (defined wantarray) ? ( wantarray ? 'A' : 'S' ) : 'V';
};
eval $qcontext;
print $q eq 'V' ? "ok 10\n" : "not ok 10\n";
$a = eval $qcontext;
print $q eq 'S' ? "ok 11\n" : "not ok 11\n";
@@a = eval $qcontext;
print $q eq 'A' ? "ok 12\n" : "not ok 12\n";
@

