@#!/l/local/bin/perl -w

$pat = '';
# construct word list
while (<DATA>) {
  chomp;
  next unless $_;
  $pat .= "$_|";
}
chop $pat if $pat =~ /\|$/;

# grep
while (<>) {
    print "$ARGV\:$.\:$_" if s/\b($pat)\b/#$1#/og;
# this variant might useful if the transformation is more complicated
#    if (/^(.*?)\b($pat)\b(.*)$/o) {
#        my $head = "$1#$2#";
#        $_ = $3;
#        while (/^(.*?)\b($pat)\b(.*)$/o) {
#            $head .= "$1#$2#";
#            $_ = $3;
#        }
#	print "$ARGV\:$.\:$head$_\n";
#    }
}
continue {
    close ARGV if eof;
}
__END__
