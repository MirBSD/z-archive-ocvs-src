AC_DEFUN([gl_FUNC_READLINK],
[
  AC_CHECK_FUNCS(readlink)
  if test $ac_cv_func_readlink = no; then
    AC_LIBOBJ(readlink)
    gl_PREREQ_READLINK
  fi
])

