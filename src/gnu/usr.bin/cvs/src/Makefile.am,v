SHELL = /bin/sh

AM_CPPFLAGS = -I$(top_srcdir)/lib -I$(top_builddir)/lib \
              -I$(top_srcdir)/diff $(ZLIB_CPPFLAGS)

bin_PROGRAMS = cvs

# The cvs executable
cvs_SOURCES = \
	add.c \
	admin.c \
	annotate.c \
	buffer.c \
	checkin.c \
	checkout.c \
	classify.c \
	client.c \
