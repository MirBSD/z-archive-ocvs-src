done

echo "$as_me:$LINENO: checking for GNU libc compatible realloc" >&5
echo $ECHO_N "checking for GNU libc compatible realloc... $ECHO_C" >&6
if test "${ac_cv_func_realloc_0_nonnull+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
