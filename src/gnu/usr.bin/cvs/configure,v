sed 's/^/| /' conftest.$ac_ext >&5

gt_cv_func_gnugettext1_libintl=no
fi
rm -f conftest.err conftest.$ac_objext \
      conftest$ac_exeext conftest.$ac_ext
                        if test "$gt_cv_func_gnugettext1_libintl" != yes && test -n "$LIBICONV"; then
