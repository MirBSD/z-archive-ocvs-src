_ACEOF


if test $ac_cv_func_lstat_dereferences_slashed_symlink = no; then
  case $LIBOBJS in
    "lstat.$ac_objext"   | \
  *" lstat.$ac_objext"   | \
    "lstat.$ac_objext "* | \
  *" lstat.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS lstat.$ac_objext" ;;
esac

fi


  echo "$as_me:$LINENO: checking for uint32_t" >&5
echo $ECHO_N "checking for uint32_t... $ECHO_C" >&6
