  # This hack won't be needed after gnulib requires Autoconf 2.58 or later.
  case " $LIBOBJS " in
  *" getndelim2.$ac_objext "* ) ;;
  *) case $LIBOBJS in
    "getndelim2.$ac_objext"   | \
