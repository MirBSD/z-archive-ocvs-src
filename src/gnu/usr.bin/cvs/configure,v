/* end confdefs.h.  */
$ac_includes_default
int
main ()
{
if ((ptrdiff_t *) 0)
  return 0;
if (sizeof (ptrdiff_t))
  return 0;
  ;
  return 0;
}
_ACEOF
rm -f conftest.$ac_objext
if { (eval echo "$as_me:$LINENO: \"$ac_compile\"") >&5
