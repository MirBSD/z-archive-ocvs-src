  *) LIBOBJS="$LIBOBJS printf-parse.$ac_objext" ;;
esac

    case $LIBOBJS in
    "asnprintf.$ac_objext"   | \
  *" asnprintf.$ac_objext"   | \
    "asnprintf.$ac_objext "* | \
  *" asnprintf.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS asnprintf.$ac_objext" ;;
