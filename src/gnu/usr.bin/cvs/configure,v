    ac_cv_path_MKTEMP="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_path_MKTEMP" && ac_cv_path_MKTEMP="mktemp"
  ;;
esac
fi
