$(srcdir)/version-client.texi: @@MAINTAINER_MODE_TRUE@@ $(srcdir)/stamp-1
$(srcdir)/stamp-1: cvsclient.texi $(top_srcdir)/configure
	@@(dir=.; test -f ./cvsclient.texi || dir=$(srcdir); \
	set `$(SHELL) $(srcdir)/mdate-sh $$dir/cvsclient.texi`; \
