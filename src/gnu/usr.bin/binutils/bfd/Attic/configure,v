  INSTALL_LIBBFD_TRUE=
  INSTALL_LIBBFD_FALSE='#'
else
  INSTALL_LIBBFD_TRUE='#'
  INSTALL_LIBBFD_FALSE=
fi
  # libbfd.a is a host library containing target dependent code
