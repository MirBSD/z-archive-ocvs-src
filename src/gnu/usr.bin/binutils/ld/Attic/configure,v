a698 62
echo $ac_n "checking for Cygwin environment""... $ac_c" 1>&6
echo "configure:542: checking for Cygwin environment" >&5
if eval "test \"$(echo '$''{'ac_cv_cygwin'+set}')\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  cat > conftest.$ac_ext <<EOF
#line 547 "configure"
#include "confdefs.h"

int main() {

#ifndef __CYGWIN__
#define __CYGWIN__ __CYGWIN32__
#endif
return __CYGWIN__;
; return 0; }
EOF
if { (eval echo configure:558: \"$ac_compile\") 1>&5; (eval $ac_compile) 2>&5; }; then
  rm -rf conftest*
  ac_cv_cygwin=yes
