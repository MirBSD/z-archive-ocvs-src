send "set variable v_char=127\n"
expect {
    -re "$prompt $"\
	{
	send "print v_char\n"
	expect {
	    -re ".\[0-9\]* = 127 \'.177\'.*$prompt $"       { pass "set variable char=127 (8-bit)" }
	    -re "$prompt $"                      { fail "set variable char=127 (8-bit)" }
	    timeout                              { fail "(timeout) set variable char=127 (8-bit)" }
	}
    }
    -re "$prompt $"                              { fail "set variable char=127 (8-bit)" }
    timeout                                      { fail "(timeout) set variable char=127 (8-bit)" }
}                         


