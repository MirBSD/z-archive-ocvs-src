SUBDIRS = @@subdirs@@

# For now, shortcut the "configure GDB for fewer languages" stuff.
YYFILES = c-exp.c \
	objc-exp.c \
	ada-exp.c \
	jv-exp.c \
	f-exp.c m2-exp.c p-exp.c
