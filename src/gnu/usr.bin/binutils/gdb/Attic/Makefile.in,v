	$(LIBGUI) $(ITCL_DEPS) $(ITK_DEPS) $(TK_DEPS) $(TCL_DEPS)
SUBDIR_GDBTK_LDFLAGS=
SUBDIR_GDBTK_CFLAGS= -DGDBTK

CONFIG_OBS= @@CONFIG_OBS@@
CONFIG_SRCS= @@CONFIG_SRCS@@
CONFIG_DEPS= @@CONFIG_DEPS@@
