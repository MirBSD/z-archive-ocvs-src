@/* $MirBSD: src/gnu/usr.bin/binutils/gdb/defs.h,v 1.3 2004/06/11 15:51:39 tg Exp $ */

/* *INDENT-OFF* */ /* ATTR_FORMAT confuses indent, avoid running it for now */
