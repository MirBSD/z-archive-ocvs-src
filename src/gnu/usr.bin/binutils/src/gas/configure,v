  echo "$ac_t""yes" 1>&6
  lt_cv_dlopen="dlopen" lt_cv_dlopen_libs="-ldl"
else
  echo "$ac_t""no" 1>&6

    lt_cv_dlopen="dyld"
    lt_cv_dlopen_libs=
