  lt_status=$lt_dlunknown
  cat > conftest.$ac_ext <<EOF
#line 5556 "configure"
#include "confdefs.h"

#if HAVE_DLFCN_H
#include <dlfcn.h>
#endif

#include <stdio.h>

#ifdef RTLD_GLOBAL
