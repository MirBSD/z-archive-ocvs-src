s%@@MAINTAINER_MODE_FALSE@@%$MAINTAINER_MODE_FALSE%g
s%@@MAINT@@%$MAINT%g
s%@@HDEFINES@@%$HDEFINES%g
s%@@CC_FOR_BUILD@@%$CC_FOR_BUILD%g
