s%@@WARN_CFLAGS@@%$WARN_CFLAGS%g
s%@@YACC@@%$YACC%g
s%@@LEX@@%$LEX%g
s%@@LEXLIB@@%$LEXLIB%g
s%@@CPP@@%$CPP%g
s%@@LEX_OUTPUT_ROOT@@%$LEX_OUTPUT_ROOT%g
