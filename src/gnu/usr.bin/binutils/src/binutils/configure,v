echo $ac_n "checking for alloca""... $ac_c" 1>&6
echo "configure:10388: checking for alloca" >&5
if eval "test \"\${ac_cv_func_alloca_works+set}\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
