#line 9337 "configure"
#include "confdefs.h"
#include <assert.h>
#include <stdio.h>
int main() {

/* check for requoting problems */
static int a, b, c, d;
static char *s;
assert (!strcmp(s, "foo bar baz quux"));
/* check for newline handling */
assert (a == b
        || c == d);

; return 0; }
EOF
