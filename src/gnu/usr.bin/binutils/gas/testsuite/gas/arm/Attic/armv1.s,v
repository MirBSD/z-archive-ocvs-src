entry:
	and	r0, r0, r0
	ands	r0, r0, r0
	eor	r0, r0, r0
	eors	r0, r0, r0
	sub	r0, r0, r0
	subs	r0, r0, r0
	rsb	r0, r0, r0
	rsbs	r0, r0, r0
	add	r0, r0, r0
