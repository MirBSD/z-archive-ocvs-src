	bics	r0, r0, r0

	tst	r0, r0
	tsts	r0, r0
	tstp	r0, r0
	teq	r0, r0
	teqs	r0, r0
	teqp	r0, r0
	cmp	r0, r0
	cmps	r0, r0
	cmpp	r0, r0
	cmn	r0, r0
	cmns	r0, r0
	cmnp	r0, r0

	mov	r0, r0
	movs	r0, r0
	mvn	r0, r0
	mvns	r0, r0

	swi	#0

	ldr	r0, [r0, #-0]
	ldrb	r0, [r0, #-0]
	ldrt	r0, [r1]
	ldrbt	r0, [r1]
	str	r0, [r0, #-0]
	strb	r0, [r0, #-0]
	strt	r0, [r1]
	strbt	r0, [r1]

	stmia	r0, {r0}
	stmib	r0, {r0}
	stmda	r0, {r0}
	stmdb	r0, {r0}
	stmfd	r0, {r0}
	stmfa	r0, {r0}
