	ldmib	r0, {r0}
	ldmda	r0, {r0}
	ldmdb	r0, {r0}
	ldmfd	r0, {r0}
	ldmfa	r0, {r0}
	ldmea	r0, {r0}
	ldmed	r0, {r0}

	# Add three nop instructions to ensure that the
	# output is 32-byte aligned as required for arm-aout.
	nop
	nop
	nop
