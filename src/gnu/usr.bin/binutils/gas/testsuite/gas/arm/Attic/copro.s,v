text
@a24 21

	@@ The following patterns test Addressing Mode 5 "Unindexed"
	
        ldc     3,   c7, [r0], {0}
        stc     p14, c6, [r1], {1}
        ldc2    5,   c5, [r2], {2}
        stc2    p6,  c4, [r3], {3}
        ldcl    7,   c3, [r4], {4}
        stcl    p8,  c2, [r5], {5}
        ldc2l   9,   c1, [r6], {6}
        stc2l   p10, c0, [r7], {7}
        ldcl    11,  c8, [r8], {255}
        stcl    p12, c9, [r9], {254}
        mrrc    13,   0, r7, r0, cr4
        mcrr    p14,  0, r7, r0, cr5
        mrrc    15,  15, r7, r0, cr15
        mcrr    p14, 15, r7, r0, cr14

	# Extra instructions to allow for code alignment in arm-aout target.
	nop
	nop
@


