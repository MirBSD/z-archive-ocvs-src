	sb	$4,small_local_common+0x1a5a5($5)
	
# Several macros are handled like sb.  Sanity check them.
	sd	$4,0
	sh	$4,0
	sw	$4,0
	swc0	$4,0
	swc1	$4,0
	swc2	$4,0
	swc3	$4,0
	s.s	$f4,0
	swl	$4,0
	swr	$4,0

# Round to a 16 byte boundary, for ease in testing multiple targets.
	nop
	nop
