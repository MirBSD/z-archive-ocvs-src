# Man page generation from texinfo
addr2line.1:	$(binutils_TEXI)
	touch $@@
	-$(TEXI2POD) $(MANCONF) -Daddr2line < $> > addr2line.pod
	-($(POD2MAN) addr2line.pod | sed -e '/^.if n .na/d' > $@@.T$$$$ && \
		mv -f $@@.T$$$$ $@@) || (rm -f $@@.T$$$$ && exit 1)
	rm -f addr2line.pod

