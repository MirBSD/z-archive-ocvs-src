#line 8294 "configure"
#include "confdefs.h"
#include <stdio.h>
int main() {
FILE *f = fopen64 ("/tmp/foo","r");
; return 0; }
EOF
if { (eval echo configure:8301: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  bu_cv_have_fopen64="need -D_LARGEFILE64_SOURCE"
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  bu_cv_have_fopen64=no
