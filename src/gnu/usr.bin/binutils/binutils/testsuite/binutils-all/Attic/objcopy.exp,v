    setup_xfail "m8*-*"
    setup_xfail "or32-*-rtems*" "or32-*-coff"
    setup_xfail "sh-*-coff*" "sh-*-rtems*" 
    setup_xfail "tic4x-*-*" "tic80-*-*" "w65-*" "z8*-*"

    clear_xfail "hppa*64*-*-hpux*" "hppa*-*-linux*" "hppa*-*-lites*"
    clear_xfail "hppa*-*-*n*bsd*" "hppa*-*-rtems*" "*-*-*elf*" "m68*-*-sysv4*"
