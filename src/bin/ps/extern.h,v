struct nlist;
struct var;
struct varent;

extern fixpt_t ccpu;
extern int eval, fscale, mempages, nlistread, rawcpu, maxslp;
extern int sumrusage, termwidth, totwidth;
extern VAR var[];
extern VARENT *vhead;

