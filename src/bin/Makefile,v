@# $MirBSD: src/bin/Makefile,v 1.6 2004/12/09 17:01:23 tg Exp $

  SUBDIR+= cat
  SUBDIR+= chmod
  SUBDIR+= cp
  SUBDIR+= date
  SUBDIR+= dd
  SUBDIR+= df
  SUBDIR+= echo
  SUBDIR+= ed
  SUBDIR+= expr
  SUBDIR+= hostname
  SUBDIR+= kill
  SUBDIR+= ksh
  SUBDIR+= ln
  SUBDIR+= ls
  SUBDIR+= md5
  SUBDIR+= mkdir
  SUBDIR+= mt
  SUBDIR+= mv
  SUBDIR+= pax
  SUBDIR+= ps
  SUBDIR+= pwd
  SUBDIR+= rm
  SUBDIR+= rmdir
  SUBDIR+= sleep
  SUBDIR+= stty
  SUBDIR+= sync
  SUBDIR+= systrace
  SUBDIR+= test

SUBDIR_ONLY_BSD+=	systrace

.include <bsd.subdir.mk>
@


