usb*)
	[ "$i" = "usb" ] && u= || u=$U
	M usb$u c 61 $U 660
a483 25
pty*)
	if [ $U -gt 15 ]; then
		echo bad unit for pty in: $i
		continue
	fi
	set -A tbl p q r s t u v w x y z P Q R S T
	name=${tbl[$U]}
	n=0
	while [ $n -lt 16 ]
	do
		nam=$name$(hex $n)
		off=$(($U*16))
		M tty$nam c 5 $(($off+$n))
		M pty$nam c 6 $(($off+$n))
		n=$(($n+1))
	done
	;;

tty[0-7][0-9a-f])
	U=${i#tty*}
	o=$(h2d $U)
	M tty$U c 8 $o 660 dialer uucp
	M cua$U c 8 $(($o+128)) 660 dialer uucp
	;;

