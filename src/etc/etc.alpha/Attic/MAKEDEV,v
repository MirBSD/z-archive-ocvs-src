music*)
	M music$U     c 42 $U
	M sequencer$U c 42 $(($U+128))
	MKlist="$MKlist;[ -e music ] || ln -s music$U music"
	MKlist="$MKlist;[ -e sequencer ] || ln -s sequencer$U sequencer"
