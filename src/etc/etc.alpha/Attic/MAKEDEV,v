lkm)
	M lkm c 16 0 640 _lkm
a337 6
fd)
	RMlist="mkdir -p fd;$RMlist" n=0
	while [ $n -lt 64 ];do M fd/$n c 10 $n;n=$(($n+1));done
	MKlist="$MKlist;chmod 555 fd"
	;;

