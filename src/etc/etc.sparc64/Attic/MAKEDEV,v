tun*)
	M tun$U c 111 $U 600
	;;

systrace)
	M systrace c 50 0 644
	;;

ss*)
	M ss$U c 59 $(($U*16)) 640 operator
	M nss$U c 59 $(($(($U*16))+1)) 640 operator
	M enss$U c 59 $(($(($U*16))+3)) 640 operator
	RMlist="$RMlist scan$U"
	MKlist="$MKlist;umask 77;ln -s ss$U scan$U"
