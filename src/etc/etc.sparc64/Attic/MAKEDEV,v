ses*)
	M ses$U c 4 $U 640 operator
	;;

*random)
	n=0
	for pre in " " s u p a
	do
		M ${pre}random c 119 $n 644
		n=$(($n+1))
	done
	;;

pf*)
	M pf c 73 0 600
	;;

