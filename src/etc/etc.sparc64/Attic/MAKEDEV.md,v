dnl
ramdisk)
	_recurse std fd0 wd0 wd1 wd2 sd0 sd1 sd2 rd0
	_recurse st0 cd0 bpf0 random
	;;

_std(2, 3, 76, 7, 16)
	M mouse		c 13 0 666
	M fb		c 22 0 666
	M openprom	c 70 0 640 kmem
	;;

mouse*)
	name=${i##mouse-}
	if [ ! -c $name ]; then
		$0 $name	# make the appropriate device
	fi
	RMlist="$RMlist mouse"
	MKlist="$MKlist;ln -s $name mouse"
	;;
