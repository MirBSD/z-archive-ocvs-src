		M cua$u c 12 $(($n+128)) 660 dialer uucp
d368 2
a369 12
st*)
	n=$(($U*16))
	for pre in " " n e en
	do
		M ${pre}st$U	b 7 $n 660 operator
		M ${pre}rst$U	c 20 $n 660 operator
		n=$(($n+1))
	done
	;;

ch*)
	M ch$U c 44 $U 660 operator
